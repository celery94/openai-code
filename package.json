{
  "name": "openai-code",
  "displayName": "OpenAI Code Chat Assistant",
  "description": "",
  "version": "0.0.2",
  "publisher": "celeryliu94",
  "license": "MIT",
  "icon": "resources/extensionIcon.png",
  "repository": {
    "url": "https://github.com/celery94/openai-code"
  },
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "copilot",
    "openai",
    "ai",
    "explain",
    "find bugs",
    "explain ",
    "refactor"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "openai-code.addComments",
        "title": "OpenAI Code Chat Assistant - Add Comments"
      },
      {
        "command": "openai-code.refactorCode",
        "title": "OpenAI Code Chat Assistant - Refactor Code"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "openai-code.addComments",
          "when": "editorHasSelection",
          "group": "openai-code-menu-group@1"
        },
        {
          "command": "openai-code.refactorCode",
          "when": "editorHasSelection",
          "group": "openai-code-menu-group@2"
        }
      ]
    },
    "configuration": {
      "title": "OpenAI Code Chat Assistant",
      "type": "object",
      "properties": {
        "openAICode.apiKey": {
          "type": "string",
          "description": "The key is used to access your Open AI API.",
          "order": 1
        },
        "openAICode.endpoint": {
          "type": "string",
          "description": "The endpoint is used to access your Open AI API.",
          "order": 2,
          "default": "https://TODO.openai.azure.com/openai/deployments/gpt-35-turbo/completions?api-version=2022-12-01"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.76.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@vscode/test-electron": "^2.2.3",
    "@vscode/vsce": "^2.18.0",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "axios": "^1.3.4"
  }
}
